def search(self, nums, target):
        s = 0 
        e =len(nums)-1
        # if len(nums) == 1 and nums[0]==target:
        #     return 0

        while s<= e:

            middle = (s+e) //2

            if nums[middle]> target:
                e= middle -1

            elif nums[middle]< target :
                s= middle + 1

            else :
                return middle 
        return -1 
        
    
